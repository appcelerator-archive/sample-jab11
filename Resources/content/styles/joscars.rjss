    textAlign: 'center',
    color: '#900',
    font: { fontSize: 18, fontWeight: 'bold' },
    height: 'auto',
    top: 20, right: 20, left: 20
}
.JOSCARsGroup {
    top: 15,
    left: 15,
    textAlign: 'left',
