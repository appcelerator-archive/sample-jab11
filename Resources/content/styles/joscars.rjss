#JOSCARsBannerImage {
    image: 'content/images/joscars/joscars.png',
    width: 105, height: 141
}
#JOSCARsScroll {
    contentHeight: 'auto',
    layout: 'vertical',
    showVerticalScrollIndicator: true,
    top: 43, left: 0, right: 0
}
#JOSCARsSubtitle {
    text: 'Nominate your favourites from the Joomla!Sphere:',
    textAlign: 'center',
    color: '#555',
    font: { fontSize: 13 },
    height: 26
}
#JOSCARsPassed {
